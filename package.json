{
  "name": "@yigityesilpinar/perfanalyticsapi",
  "version": "1.0.0",
  "description": "a restful API which saves data, posted from PerfAnalytics.JS and returns time specific filtered data.",
  "license": "ISC",
  "author": "yigityesilpinar@gmail.com",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc --project ./",
    "db:local": "npm run db:prepare && sleep 1 && npm run db:migrate && npm run db:seedAll",
    "db:migrate": "sequelize db:migrate",
    "db:prepare": "docker-compose stop && docker-compose rm -f && docker-compose up --force-recreate -d",
    "db:seedAll": "sequelize db:seed:all",
    "dev": "cross-env NODE_ENV=development nodemon src/app/index.ts",
    "lint": "eslint --ext .tsx,.ts,.js,.jsx .",
    "production": "node dist/src/app",
    "sort": "npx sort-package-json",
    "swagger": "ts-node ./scripts/swagger",
    "test": "npm run db:local && cross-env NODE_ENV=test TS_NODE_PROJECT=\"tsconfig.json\"  mocha --exit -r ts-node/register 'tests/**/*.ts'",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "compression": "1.7.4",
    "convict": "6.2.1",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "express-validator": "6.14.0",
    "helmet": "4.6.0",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "sequelize": "6.12.0-alpha.1",
    "sequelize-typescript": "2.1.1",
    "swagger-jsdoc": "6.1.0",
    "swagger-ui-express": "4.2.0"
  },
  "devDependencies": {
    "@types/autocannon": "4.1.1",
    "@types/chai": "4.3.0",
    "@types/compression": "1.7.2",
    "@types/convict": "6.1.1",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/mocha": "9.0.0",
    "@types/node": "14.17.3",
    "@types/sequelize": "4.28.10",
    "@types/supertest": "2.0.11",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@types/validator": "13.7.0",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "autocannon": "7.5.0",
    "chai": "4.3.4",
    "eslint": "8.4.0",
    "eslint-plugin-typescript": "0.14.0",
    "install": "0.13.0",
    "mocha": "9.1.3",
    "nodemon": "2.0.15",
    "npm": "8.3.0",
    "sequelize-cli": "6.3.0",
    "supertest": "6.1.6",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  }
}
